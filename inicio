<script type="text/javascript">
	



(function() {
  // VARIABLES   ------------------------------------------
  const productsContainer = document.querySelector("#grid");
  const cartContainer = document.querySelector("#shopping-cart");
  const cartContent = document.querySelector("#cart-content");
  const toggleCartBtn = document.querySelector("#toggle-cart-btn");
  const clearCartBtn = document.querySelector("#clear-cart");
  const checkoutBtn = document.querySelector("#checkout-btn");
  const totalPriceContainer = document.querySelector("#total-price");
// VARIABLES   ------------------------------------------

  // FUNCIONES  ---------------------------------------

  function toggleCart() {
    // alternar la visibilidad del carrito de compras
    cartContainer.classList.toggle("open");
  }

  function getLSContent() {
    //obtener contenido del almacenamiento local.
    // si no hay nada, crea una matriz vacía
    const lsContent = JSON.parse(localStorage.getItem("products")) || [];
    return lsContent;
  }

  function setLSContent(lsContent) {
    // guardar contenido dentro del almacenamiento local
    localStorage.setItem("products", JSON.stringify(lsContent));
  }

  function calculateTotal(prices) {
   
// calcular el precio total en el carrito
    return prices.reduce(function(prev, next) {
      return prev + next;
    }, 0);
  }

  function getCartItemPrices() {
    // extrae los números de precio de los artículos del carrito para calcular el total
    const prices = [];
    // recuperar el elemento td en el carrito donde se almacena el precio del producto
    // por cada producto en el carrito
    let nums = cartContent.querySelectorAll("tr td:nth-child(3)");

   // iterar sobre cada nodo td y extraer el precio
    // quitar el signo $ del texto, convertir la cadena en
    // un número y empujar el número en la matriz de precios
    if (nums.length > 0) {
      for (let cell = 0; cell < nums.length; cell++) {
        let num = nums[cell].innerText;
        num = num.replace(/[^\d]/g, "");
        num = parseFloat(num);
        prices.push(num);
      }
      // devuelve la matriz de precios
      return prices;
    } else {
      return;
    }
  }

  function displayCartTotal() {
   // mostrar el costo total en el carrito
    const prices = getCartItemPrices();
    let total = 0;
    if (prices) {
      total = calculateTotal(prices);
      totalPriceContainer.innerHTML = `<span class="total">Total: $${total.toFixed(
        2
      )}</span>`;
    } else {
      totalPriceContainer.innerHTML = '<span class="total">Total: $0</span>';
    }
  }

  function displayProducts() {
    // mostrar todos los productos en el carrito

    // obtener contenido del almacenamiento local
    const lsContent = getLSContent();
    let productMarkup = "";
    
	// si el almacenamiento local no está vacío, construye el
    // marcado de artículos del carrito y los detalles apropiados
    if (lsContent !== null) {
      for (let product of lsContent) {
        productMarkup += `
          <tr>
          <td><img class="cart-image" src="${product.image}" alt="${
          product.name
        }" width="120"></td>
          <td>
            ${product.name}
          </td>
          <td>${product.price}</td>
          <td><a href="#" data-id="${product.id}" class="remove">X</a></td>
          </tr>
        `;
      }
    } else {
      
// si no hay contenido en el almacenamiento local, alertar al usuario
      productMarkup = "Tu Carrito esta vacio.";
    }
    // agregar marcado a DOM
    cartContent.querySelector("tbody").innerHTML = productMarkup;
  }

  function saveProduct(clickedBtn) {
   // guardar el producto seleccionado en el almacenamiento local y mostrarlo en el carrito juntos

    // variables
    const productId = clickedBtn.getAttribute("data-id");
    const card = clickedBtn.parentElement.parentElement;
    const cardInfo = clickedBtn.parentElement;
    const prodImage = card.querySelector("img").src;
    const prodName = cardInfo.querySelector("h4").textContent;
    const prodPrice = cardInfo.querySelector(".card__price").textContent;

    let isProductInCart = false;

    // obtener matriz de almacenamiento local
    const lsContent = getLSContent();

   // para evitar que el usuario agregue el mismo curso dos veces, verifique
    // el producto aún no está en LS antes de agregarlo
    lsContent.forEach(function(product) {
      if (product.id === productId) {
        alert("Este producto ya esta en tu carrito.");
        isProductInCart = true;
      }
    });

   // solo si el producto no está ya en el carrito,
    // crea un objeto que representa la información del producto seleccionado
    // y empujarlo a la matriz de almacenamiento local
    if (!isProductInCart) {
      lsContent.push({
        id: productId,
        image: prodImage,
        name: prodName,
        price: prodPrice
      });

      // agregar el producto al almacenamiento local
      setLSContent(lsContent);
      // actualizar la visualización de cursos en el carrito de compras
      displayProducts();
    }
  }

  function removeProduct(productId) {
    // eliminar el producto del carrito (y del almacenamiento local)

    // recupera la lista de productos de LS
    const lsContent = getLSContent();

   
// obtener el índice del elemento del producto para eliminar
    // dentro de la matriz de contenido de almacenamiento local
    let productIndex;
    lsContent.forEach(function(product, i) {
      if (product.id === productId) {
        productIndex = i;
      }
    });

   // modificar los elementos en la matriz de almacenamiento local
    // para eliminar el elemento del producto seleccionado

    lsContent.splice(productIndex, 1);
    // actualizar el contenido del almacenamiento local
    setLSContent(lsContent);

    displayProducts();
  }

  function clearCart() {
   // borrar todos los productos del carrito (y del almacenamiento local)

    // recupera la lista de productos de LS
    const lsContent = getLSContent();
    // matriz vacía en el almacenamiento local
    lsContent.splice(0, lsContent.length);
    // actualizar el almacenamiento local
    setLSContent(lsContent);
   // mostrar el contenido del carrito de nuevo
    displayProducts();
  }

  function checkout() {
    // pago: simplemente borre el carrito
    // después de que el usuario confirme el proceso de pago
    const cartProducts = cartContent.querySelector("tbody").innerHTML;
    if (cartProducts !== "" && confirm("¿Estás seguro de que quieres pagar?")) {
      clearCart();
    } else {
      return;
    }
  }

 
// ENLACE DE EVENTOS Y FUNCIONES DE LLAMADA

  // Carga de la página:
  document.addEventListener("DOMContentLoaded", function(e) {
   // mostrar la lista de productos en el carrito, si los hay, al cargar la página
    displayProducts();
   // mostrar el total del carrito
    displayCartTotal();
  });

  // abrir y cerrar carrito de compras
  // cuando se hace clic en el botón del carrito
  toggleCartBtn.addEventListener("click", function(e) {
    e.preventDefault();
    toggleCart();
  });

  // Guardar producto en carrito y Almacenamiento Local
  // cuando se hace clic en el botón Agregar al carrito
  productsContainer.addEventListener("click", function(e) {
    if (e.target.classList.contains("add-to-cart")) {
      e.preventDefault();
      const clickedBtn = e.target;
      saveProduct(clickedBtn);
    }
  });

  productsContainer.addEventListener("click", function(e) {
    if (e.target.classList.contains("add-to-cart")) {
      displayCartTotal();
    }
  });

  // vincular removeProduct para hacer clic en el evento de la tabla cartContent
  cartContent.querySelector("tbody").addEventListener("click", function(e) {
    e.preventDefault();
   
// identificar el botón en el que se hizo clic
    const clickedBtn = e.target;
    // si es un boton de eliminar
    if (e.target.classList.contains("remove")) {
      // obtener el valor de la propiedad data-id, que contiene el
      // id del producto seleccionado
      const productId = clickedBtn.getAttribute("data-id");
     // obtener el valor de la propiedad data-id, que contiene el
      // id del producto seleccionado
      removeProduct(productId);
      // mostrar productos en el carrito nuevamente,
      // ahora la lista debería mostrarse con 1 producto menos
      // o vacío si no quedan productos en el carrito

      // Ajustar el total
      displayCartTotal();
    }
  });

  // vincular el botón para vaciar el carrito tanto a la función que
  // borra el carrito y a la función que ajusta el precio total
  clearCartBtn.addEventListener("click", function(e) {
    e.preventDefault();
    clearCart();
  });
  clearCartBtn.addEventListener("click", displayCartTotal);

  
// vincular el botón que realiza el pago a la función que
  // controla el pago y la función que ajusta el precio total
  checkoutBtn.addEventListener("click", function(e) {
    e.preventDefault();
    checkout();
  });
  checkoutBtn.addEventListener("click", displayCartTotal);
})();



// FUNCIONES  ---------------------------------------



</script>
